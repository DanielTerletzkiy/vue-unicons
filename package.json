{
  "name": "vue-unicons",
  "version": "1.0.0",
  "private": false,
  "keywords": [
    "vue",
    "vuejs",
    "vue-component",
    "vue-icons",
    "components",
    "icons"
  ],
  "main": "dist/vue-unicons.common.js",
  "license": "MIT",
  "author": "Anton Reshetov",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run test:unit && vue-cli-service build --target lib --name vue-unicons ./src/components/index.js",
    "build:docs": "vue-cli-service build --dest docs",
    "generate": "node ./build/generate.js",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "unicons": "github:iconscout/unicons#master",
    "vue": "^2.5.22"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-unit-jest": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.3",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
